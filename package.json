{
  "name": "unee-t-fe",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "npm run lint && npm run proof",
    "debug": "meteor run --inspect",
    "lint": "standard --verbose | snazzy",
    "lint-fix": "standard --fix",
    "proof": "TEST_BROWSER_DRIVER=puppeteer meteor test --once --driver-package meteortesting:mocha",
    "test-local": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha",
    "flow": "flow"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "hammerjs": "^2.0.8",
    "history": "^4.6.3",
    "iso-3166-1-codes": "^1.0.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.2",
    "memoize-one": "^5.0.0",
    "meteor-node-stubs": "~0.4.1",
    "moment": "^2.19.3",
    "prop-types": "^15.5.10",
    "rand-token": "^0.4.0",
    "react": "^16.7.0",
    "react-addons-pure-render-mixin": "^15.6.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-swipeable-views": "^0.13.0",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^6.3.3",
    "signature_pad": "^3.0.0-beta.3",
    "tracer": "^0.9.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "enzyme": "^3.8.0",
    "eslint": "^5.4.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "flow-bin": "^0.89.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-modules-extract-imports": "^2.0.0",
    "postcss-modules-local-by-default": "^2.0.2",
    "postcss-modules-scope": "^2.0.1",
    "postcss-nested": "^4.1.1",
    "puppeteer": "^1.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^16.7.0",
    "snazzy": "^8.0.0",
    "spacejam": "^1.6.1",
    "standard": "^12.0.1"
  },
  "cssModules": {
    "cssClassNamingConvention": {
      "replacements": [
        "_client__main__"
      ]
    },
    "postcssPlugins": {
      "postcss-easy-import": {},
      "postcss-nested": {},
      "postcss-modules-local-by-default": {},
      "postcss-modules-extract-imports": {},
      "postcss-modules-scope": {},
      "autoprefixer": {
        "inlineOptions": {
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    }
  }
}
